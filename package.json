{
  "name": "about",
  "version": "1.0.11",
  "main": "index.js",
  "scripts": {
    "build": "node ./bin/cli build",
    "clean": "rm -rf dist",
    "dev": "linklocal link -r && linklocal list -r | bulk -c 'npm i --production'",
    "postversion": "npm run build && gh-pages -d ./dist && npm run clean && echo 'done'",
    "prepublish": "if [[ $NODE_ENV != 'production' ]]; then npm run dev; fi",
    "preversion": "npm run clean",
    "start": "node ./bin/cli start -p 1337 | garnish",
    "test": "standard && NODE_ENV=test node test",
    "test-cov": "standard && NODE_ENV=test istanbul cover test.js"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "@about/background-video": "file:background-video",
    "css-wipe": "^1.0.1",
    "hash-match": "^1.0.1",
    "wayfarer": "^4.1.1",
    "webcomponents.js": "^0.7.2"
  },
  "devDependencies": {
    "babelify": "^6.1.2",
    "brick-router": "^2.1.1",
    "brick-server": "^1.0.0",
    "browserify": "^10.2.3",
    "bulk": "^2.0.0",
    "cliclopts": "^1.1.0",
    "cssnext": "^1.6.0",
    "garnish": "^2.1.3",
    "gh-pages": "^0.3.0",
    "istanbul": "^0.3.14",
    "linklocal": "^2.5.2",
    "minimist": "^1.1.1",
    "server-summary": "^2.0.2",
    "standard": "^4.0.1",
    "tape": "^4.0.0",
    "watchify": "^3.2.1"
  }
}
